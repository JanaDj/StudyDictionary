<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meangirl.dictionary">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" >
    </uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/studyicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activities.BrowseContent"/>
        <activity android:name=".Activities.FlashCards"/>
        <activity android:name=".Activities.Quiz"/>
        <activity android:name=".Activities.ManageData"/>
        <activity android:name=".FileManager"/>


    </application>
</manifest>